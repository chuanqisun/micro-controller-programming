  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/chuanqisun/micro-controller-programming/issues"
  },
  "homepage": "https://github.com/chuanqisun/micro-controller-programming#readme",
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "workspaces": [
    "apps/*"
  ],
  "dependencies": {
    "form-data": "^4.0.4",
    "openai": "^6.7.0",
    "ws": "^8.18.3"
  }
}
