  "scripts": {
    "dev": "npm run build:sound-test && npm run upload:sound-test && npm run monitor",
    "dev:rp2040": "npm run build:rp2040 && npm run upload:rp2040 && npm run monitor",
    "build:rp2040": "arduino-cli compile -b rp2040:rp2040:seeed_xiao_rp2040 ./sketches/cube-viewer",
    "upload:rp2040": "arduino-cli upload -p /dev/ttyACM0 -b rp2040:rp2040:seeed_xiao_rp2040 ./sketches/cube-viewer",
    "build:esp32": "arduino-cli compile -b arduino:esp32:nano_nora ./sketches/esp32",
    "upload:esp32": "arduino-cli upload -p /dev/ttyACM0 -b arduino:esp32:nano_nora ./sketches/esp32",
    "build:sound-test": "arduino-cli compile -b esp32:esp32:nano_nora ./sketches/sound-test",
    "upload:sound-test": "arduino-cli upload -p /dev/ttyACM0 -b esp32:esp32:nano_nora ./sketches/sound-test",
    "monitor": "arduino-cli monitor -p /dev/ttyACM0 -c baudrate=115200"
  },
